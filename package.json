{
  "name": "thunder-schema",
  "version": "1.0.19",
  "description": "A TypeScript ORM for ClickHouse with migrations support",
  "files": [
    "dist/index.js",
    "dist/cli/index.js",
    "dist/index.d.ts",
    "README.md"
  ],
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "thunder-schema": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup && chmod +x dist/cli/index.js",
    "clean": "rimraf dist",
    "test": "jest --forceExit --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "makemigrations": "ts-node src/cli/index.ts",
    "readmigrations": "ts-node src/cli/index.ts",
    "migrate": "ts-node src/cli/index.ts"
  },
  "keywords": [
    "clickhouse",
    "orm",
    "typescript",
    "database",
    "migrations",
    "clickhouse-orm",
    "clicksy"
  ],
  "author": "Aman Dugar",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amandugar/clickhouse-orm.git"
  },
  "bugs": {
    "url": "https://github.com/amandugar/clickhouse-orm/issues"
  },
  "homepage": "https://github.com/amandugar/clickhouse-orm#readme",
  "dependencies": {
    "@clickhouse/client": "^1.11.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "workspaces": [
    "src/*"
  ]
}